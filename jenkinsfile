pipeline {
agent any
tools {
    maven 'Manven 3.6.0'
    jdk 'jave1.8.0'
   }
   stages {
   stage('Build') {
   step {
      sh "mvn -b -dskiptests clean package"
      }
      }
      stage("Deploy") {
      steps {
        sh 'java -jar target/sample-app-1.0-snapshot.jar'
        }
       }
     }
    }
