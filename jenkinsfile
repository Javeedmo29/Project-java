pipeline {
   agent any
tools {
    maven 'Maven 3.6.0'
    jdk 'java1.8.0'
  }
   stages {
      stage('Build') {
           step {
           
           sh "mvn -B -DskipTests clean package"
         }
        }
        
        stage('Deploy') {
           step{
            sh 'java -jar target/sample-app-1.0-SNAPSHOT.jar'
            }
        }
   }
}
