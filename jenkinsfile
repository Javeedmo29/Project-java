pipeline {
   agent any
tools {
    maven 'Maven 3.6.0'
    jdk 'java1.8.0'
  }
   stages {
      stage('BUILD') {
           steps {
           
           sh "mvn -B -DskipTests clean package"
         }
        }
        stage('Deploy') {
           steps {
            sh 'java -jar target/my-app-1.0-SNAPSHOT.jar'
            }
        }
        stage('Test'){
        steps {
        sh 'mvn test'
        }
        }
   stage('Artifactory') {
           steps {
           sh 'curl -X PUT -u admin:Javeed@90 -T target/my-app-1.0-SNAPSHOT.jar "http://104.211.216.81:8081/artifactory/example-repo-local/my-app-1.0-SNAPSHOT.jar"'
            }
        }
}

}
